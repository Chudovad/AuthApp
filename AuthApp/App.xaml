<Application x:Class="AuthApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:AuthApp" 
             xmlns:views="clr-namespace:AuthApp.Views"
             xmlns:vm="clr-namespace:AuthApp.ViewModels">
    <Application.Resources>
        <ResourceDictionary>
            <DataTemplate DataType="{x:Type vm:View1ViewModel}">
                <views:View1 />
            </DataTemplate>
            <DataTemplate DataType="{x:Type vm:View2ViewModel}">
                <views:View2 />
            </DataTemplate>
            <DataTemplate DataType="{x:Type vm:AuthViewModel}">
                <views:AuthView />
            </DataTemplate>
            <DataTemplate DataType="{x:Type vm:LogoutViewModel}">
                <views:LogoutView />
            </DataTemplate>
        </ResourceDictionary>
    </Application.Resources>
</Application>
